<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="userInfo" catalog="bookManage" class="User">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>

	<table name="SystemInfo" catalog="bookManage" class="Announce">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>

	<table name="bookInfo" catalog="bookManage" class="Book">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>

	<table name="borrowInfo" catalog="bookManage" class="BorrowBook">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	
    <table name="bookIDInfo" catalog="bookManage" class="BookID">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>

</hibernate-reverse-engineering>